local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "RAINBOW BRI V0.002", HidePremium = false, SaveConfig = true, ConfigFolder = "RAINBOW BRI V0.002"})
local Tab = Window:MakeTab({
	Name = "MAIN",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "GET ALL LUCKY BLOX (WORKING)"
})

OrionLib:MakeNotification({
	Name = "OMG TY",
	Content = "THANK YOU FOR USE MY SCRIPT☺️",
	Image = "rbxassetid://4483345998",
	Time = 5
})

Tab:AddButton({
	Name = "GIVE LUCKYBLOX NORMAL",
	Callback = function()
game:GetService("ReplicatedStorage"):WaitForChild("SpawnLuckyBlock"):FireServer()
      		print("button pressed")
  	end    
})

